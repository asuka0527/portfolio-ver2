* {
  margin: 0;
  padding: 0;
  box-sizing: border-box; }

html,
body {
  background-color: #f4f2f0;
  color: #ba8b02;
  font-family: "Raleway", sans-serif;
  font-weight: 300;
  font-size: 62.5%; }

@media only screen and (max-width: 37.5em) {
    html,
    body {
      font-size: 50%; } }

.header--1 {
  font-size: 7rem;
  color: #f4f2f0;
  letter-spacing: 4px;
  font-weight: 400; }

.header--2 {
  font-size: 6rem;
  color: rgba(255, 133, 128, 0.9);
  letter-spacing: 4px;
  font-weight: 500; }

.header--3 {
  font-size: 3rem;
  letter-spacing: 2px;
  color: rgba(255, 133, 128, 0.9);
  text-transform: uppercase;
  word-spacing: 1;
  font-weight: 400; }

.header--4 {
  font-size: 2rem;
  letter-spacing: 2px;
  color: rgba(255, 133, 128, 0.9);
  text-transform: uppercase;
  word-spacing: 1;
  font-weight: 400;
  margin-bottom: 2rem; }

.btn__container {
  display: flex; }

@media only screen and (max-width: 37.5em) {
    .btn__container {
      flex-direction: column;
      align-items: flex-start; } }

.btn {
  border: none;
  background-image: linear-gradient(to left bottom, #ff8580, #ff8580);
  padding: 1rem 1.5rem;
  color: #fff;
  font-family: inherit;
  text-transform: uppercase;
  transition: all 0.2s;
  cursor: pointer;
  display: inline-block;
  margin-right: 3rem;
  font-size: 1.5rem;
  outline: none; }

@media only screen and (max-width: 37.5em) {
    .btn {
      margin-right: 2rem; } }

.btn:hover {
  background-image: linear-gradient(to left bottom, rgba(48, 67, 82, 0.1), rgba(215, 210, 204, 0.1));
  color: #304352;
  transform: translateY(-0.5rem); }

.btn-outline {
  border: none;
  padding: 1rem 1.5rem;
  color: rgba(255, 133, 128, 0.9);
  font-family: inherit;
  text-transform: uppercase;
  transition: all 0.2s;
  cursor: pointer;
  display: inline-block;
  margin-right: 3rem;
  font-size: 1.5rem;
  outline: none;
  border: 1.5 px solid rgba(255, 133, 128, 0.9);
  background: transparent; }

.btn-outline:hover {
  background-image: linear-gradient(to left bottom, rgba(255, 133, 128, 0.9), rgba(255, 133, 128, 0.9));
  color: #d7d2cc;
  transform: translateY(-0.5rem); }

.container {
  max-width: 120rem;
  margin: 0 auto; }

.header {
  height: 100vh;
  background-image: linear-gradient(rgba(0, 0, 0, 0.7), rgba(0, 0, 0, 0.7)), url(322604b6e8652cd9833b20cd0fc86c18);
  background-size: cover;
  display: flex;
  flex-direction: column; }

.section {
  padding: 4rem;
  transition: transform 1s, opacity 1s;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center; }

.section__text--about {
    color: rgba(255, 133, 128, 0.9); }

.section__text--skill {
    color: #f4f2f0; }

.section--hidden {
  opacity: 0; }

.section:last-child {
  padding: 4rem 4rem 12rem 4rem; }

.section-title {
  text-align: center;
  text-transform: uppercase;
  margin: 8rem auto; }

.section-about {
  background-image: linear-gradient(to right bottom, rgba(215, 210, 204, 0.85), rgba(244, 242, 240, 0.85));
  background-position: center;
  background-size: cover;
  width: 100vw;
  margin: 0 auto; }

.section-skill {
  background-image: linear-gradient(rgba(0, 0, 0, 0.8), rgba(0, 0, 0, 0.8)), url(f616e22fd6d82edf2f0d26f23a2e7095);
  background-position: center;
  background-size: cover;
  width: 100vw;
  margin: 0 auto;
  padding-bottom: 20rem; }

.nav__header {
  padding: 2rem;
  width: 100%;
  z-index: 1000;
  position: relative; }

@media only screen and (max-width: 37.5em) {
    .nav__header {
      padding: 2rem 1rem; } }

.nav__links {
  max-width: 120rem;
  margin: 0 auto;
  display: flex;
  list-style: none;
  justify-content: flex-end;
  align-items: center;
  transition: all 0.2s; }

@media only screen and (max-width: 37.5em) {
    .nav__links {
      justify-self: flex-end;
      flex-direction: column;
      align-items: flex-end;
      margin-right: 1rem;
      margin-top: 5rem;
      opacity: 0;
      text-align: none; } }

.nav__links .icon {
    height: 3rem;
    width: 3rem; }

.nav__link:link, .nav__link--lang:link, .nav__link:visited, .nav__link--lang:visited {
  font-size: 2rem;
  text-decoration: none;
  text-transform: uppercase;
  padding: 0 1rem;
  color: #fff;
  fill: #fff;
  transition: all 0.2s; }

@media only screen and (max-width: 37.5em) {
    .nav__link:link, .nav__link--lang:link, .nav__link:visited, .nav__link--lang:visited {
      color: rgba(255, 133, 128, 0.9);
      fill: rgba(255, 133, 128, 0.9); } }

@media only screen and (max-width: 37.5em) {
    .nav__link:link, .nav__link--lang:link, .nav__link:visited, .nav__link--lang:visited {
      display: block;
      padding-bottom: 1rem; } }

.nav__link:hover, .nav__link--lang:hover {
  color: rgba(255, 133, 128, 0.9);
  fill: rgba(255, 133, 128, 0.9); }

@media only screen and (max-width: 37.5em) {
    .nav__link:hover, .nav__link--lang:hover {
      color: #f4f2f0;
      fill: #f4f2f0; } }

.sticky {
  position: fixed;
  background-image: linear-gradient(to left bottom, rgba(215, 210, 204, 0.8), rgba(48, 67, 82, 0.8)); }

@media only screen and (max-width: 37.5em) {
    .sticky {
      background-image: none; } }

.hamburger {
  position: absolute;
  cursor: pointer;
  right: 2%; }

.hamburger .line {
    width: 30px;
    height: 3px;
    background: rgba(255, 133, 128, 0.9);
    margin: 5px; }

@media only screen and (min-width: 37.5em) {
      .hamburger .line {
        opacity: 0; } }

.hidden {
  opacity: 1; }

.color {
  background: #304352; }

.header__text {
  font-size: 8rem;
  color: #fff;
  font-weight: 400;
  position: absolute;
  top: 45%;
  left: 35%;
  transform: translate(-45%, -35%); }

.text-type {
  margin-left: 4.5rem;
  color: rgba(255, 133, 128, 0.9); }

@media only screen and (max-width: 37.5em) {
    .text-type {
      margin-top: 2rem;
      margin-left: 0; } }

.text-type > .text {
  border-right: 0.3rem solid rgba(255, 133, 128, 0.9); }

.typewriter {
  position: absolute;
  top: 50%;
  left: 10%; }

@media only screen and (max-width: 37.5em) {
    .typewriter {
      left: 5%; } }

.project__card {
  background-color: #ffffff;
  padding: 5rem;
  box-shadow: 0 1rem 4rem rgba(0, 0, 0, 0.4);
  position: relative;
  max-width: 90rem; }

@media only screen and (max-width: 50em) {
    .project__card {
      align-items: center;
      padding: 4rem; } }

.project__container {
  display: flex; }

@media only screen and (max-width: 37.5em) {
    .project__container {
      flex-direction: column;
      align-items: center; } }

.project__title {
  margin: 0 auto;
  margin-bottom: 5rem;
  text-align: center;
  color: #304352; }

.project__title::after {
  display: block;
  height: 2px;
  background-color: rgba(255, 133, 128, 0.9);
  width: 15rem;
  margin: 0 auto;
  content: " ";
  margin-top: 5px; }

.project__text {
  margin-left: 5rem;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  animation: moveInRight ease-out 2s; }

@media only screen and (max-width: 37.5em) {
    .project__text {
      margin-left: 0;
      text-align: center; } }

.project__paragraph {
  font-size: 1.6rem;
  color: #304352;
  margin-bottom: 5rem; }

@media only screen and (max-width: 37.5em) {
    .project__paragraph {
      margin-top: 5rem; } }

.project__login {
  font-size: 1.6rem;
  color: #304352;
  animation: moveInLeft ease-out 2s; }

.project .btn-project {
  text-decoration: none;
  box-shadow: 0 0.5rem 1rem rgba(0, 0, 0, 0.4);
  margin-bottom: 4rem; }

.photo {
  display: block;
  width: 40rem;
  height: auto;
  box-shadow: 0 1rem 2rem rgba(0, 0, 0, 0.4);
  animation: moveInLeft ease-out 2s; }

@media only screen and (max-width: 50em) {
    .photo {
      width: 30rem; } }

.about {
  display: flex;
  justify-content: space-between;
  width: 80%;
  margin: 0 auto;
  margin-bottom: 8rem; }

@media only screen and (max-width: 62.5em) {
    .about {
      width: 100%; } }

@media only screen and (max-width: 50em) {
    .about {
      flex-direction: column; }
      .about > * {
        margin-bottom: 5rem; } }

.about__photo {
    height: 40rem;
    width: auto;
    box-shadow: 0 0.5rem 1rem rgba(0, 0, 0, 0.4);
    animation: moveInLeft ease-out 2s; }

@media only screen and (max-width: 50em) {
      .about__photo {
        align-self: center; } }

.about__skills {
    animation: moveInRight ease-out 2s;
    margin-left: 3rem; }

.about__skills > * {
      margin-bottom: 3rem; }

@media only screen and (max-width: 37.5em) {
      .about__skills {
        margin-left: 0; } }

.about__paragraph {
    font-size: 1.8rem;
    font-weight: 400;
    color: #304352;
    line-height: 2; }

.skills {
  display: flex;
  justify-content: space-between; }

@media only screen and (max-width: 50em) {
    .skills {
      flex-direction: column;
      align-items: center; } }

.skills__container {
    width: 45rem;
    background-color: rgba(244, 242, 240, 0.8);
    padding: 3rem 4rem;
    box-shadow: 0 1rem 2rem rgba(0, 0, 0, 0.4); }

.skills__container--left {
      animation: moveInLeft ease-out 2s; }

.skills__container--right {
      animation: moveInRight ease-out 2s; }

.skills__container:first-child {
      margin-right: 10rem; }

@media only screen and (max-width: 62.5em) {
        .skills__container:first-child {
          margin-right: 5rem; } }

@media only screen and (max-width: 50em) {
        .skills__container:first-child {
          margin-right: 0; } }

@media only screen and (max-width: 62.5em) {
      .skills__container {
        width: 40rem; } }

@media only screen and (max-width: 50em) {
      .skills__container {
        width: 50rem;
        margin-bottom: 5rem; } }

@media only screen and (max-width: 37.5em) {
      .skills__container {
        width: 40rem;
        margin-bottom: 5rem; } }

.skills__title {
    text-align: center;
    margin-bottom: 4rem;
    color: #1b252e; }

.skills__title::after {
    display: block;
    height: 2px;
    background-color: rgba(255, 133, 128, 0.9);
    width: 50%;
    margin: 0 auto;
    content: " ";
    margin-top: 5px; }

.skills ul > *:not(:last-child) {
    margin-bottom: 1rem; }

.skills li {
    font-size: 2rem;
    letter-spacing: 2px;
    color: #304352;
    text-transform: uppercase;
    line-height: 1.5;
    font-weight: 400; }

.footer {
  background-image: linear-gradient(rgba(0, 0, 0, 0.8), rgba(0, 0, 0, 0.8)), url(0797e22d1c393a30266105e4e210a417);
  background-position: center;
  background-size: cover;
  padding: 10rem 2rem; }

.footer__container {
    max-width: 120rem;
    margin: 0 auto;
    display: flex;
    align-items: center;
    justify-content: space-between; }

@media only screen and (max-width: 37.5em) {
      .footer__container {
        flex-direction: column; } }

@media only screen and (max-width: 37.5em) {
  .copyright {
    margin-top: 2rem; } }

.copyright__text {
  font-size: 2rem;
  color: #fff;
  text-transform: uppercase;
  font-weight: 600;
  letter-spacing: 1px; }

.copyright__text--highlight {
  color: rgba(255, 133, 128, 0.9); }

.nav-footer__header {
  padding: 2rem;
  width: 100%;
  z-index: 1000;
  position: relative; }

@media only screen and (max-width: 37.5em) {
    .nav-footer__header {
      padding: 2rem 1rem; } }

.nav-footer__links {
  max-width: 120rem;
  margin: 0 auto;
  display: flex;
  list-style: none;
  justify-content: flex-end;
  align-items: center;
  transition: all 0.2s; }

.nav-footer__links .icon {
    height: 3rem;
    width: 3rem; }

.nav-footer__link:link, .nav-footer__link:visited {
  font-size: 2rem;
  text-decoration: none;
  text-transform: uppercase;
  padding: 0 1rem;
  color: #fff;
  fill: #fff;
  transition: all 0.2s; }

@media only screen and (max-width: 37.5em) {
    .nav-footer__link:link, .nav-footer__link:visited {
      display: block;
      padding-bottom: 1rem; } }

.nav-footer__link:hover {
  color: rgba(255, 133, 128, 0.9);
  fill: rgba(255, 133, 128, 0.9); }

@keyframes moveInLeft {
  /* initial */
  0% {
    opacity: 0;
    transform: translateX(-10rem); }
  /* end*/
  100% {
    opacity: 1;
    transform: 0; } }

@keyframes moveInRight {
  /* initial */
  0% {
    opacity: 0;
    transform: translateX(10rem); }
  /* end*/
  100% {
    opacity: 1;
    transform: 0; } }

@keyframes moveInBottom {
  /* initial */
  0% {
    opacity: 0;
    transform: translateY(30px); }
  100% {
    transform: translateY(0); } }
